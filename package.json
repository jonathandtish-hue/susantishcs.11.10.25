{
  "name": "susantish-website",
  "version": "1.0.0",
  "description": "Susan Tish Christian Science Practitioner Website",
  "scripts": {
    "build:css": "tailwindcss -i ./src/input.css -o ./website/styles.css --minify",
    "watch:css": "tailwindcss -i ./src/input.css -o ./website/styles.css --watch",
    "generate": "python3 generate_website_v5.py",
    "build": "npm run generate && npm run build:css",
    "dev": "npm run generate && npm run watch:css",
    "build:dev": "vite build --mode development"
  },
  "keywords": [
    "tailwindcss",
    "website"
  ],
  "author": "Susan Tish",
  "license": "ISC",
  "devDependencies": {
    "tailwindcss": "^4.1.17"
  },
  "dependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.22",
    "lovable-tagger": "^1.1.11",
    "postcss": "^8.5.6",
    "react-router-dom": "^7.9.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
